{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2023"],
    "module": "commonjs",
    "rootDir": "./",
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,            
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
